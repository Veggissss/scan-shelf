{
  "name": "scan-shelf",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:pages": "NEXT_PUBLIC_DEPLOY_ENVIRONMENT=pages next build",
    "start": "next start",
