    "local": "concurrently \"npm run build && npm run start\" \"npm run server\""
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.6",
    "epubjs": "^0.3.93",
    "next": "14.2.20",
    "next-pwa": "^5.6.0",
    "react": "^18",
    "react-dom": "^18",
    "react-reader": "^2.0.11",
    "react-zoom-pan-pinch": "^3.6.1",
    "use-local-storage-state": "^19.4.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.12",
    "msw": "^2.4.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
